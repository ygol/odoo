<?xml version="1.0" encoding="utf-8"?>
<odoo noupdate="1">
    <record id="ir_rule_sms_template_calendar_manager" model="ir.rule">
        <field name="name">SMS Template: calendar manager CRUD on event / registrations templates</field>
        <field name="model_id" ref="sms.model_sms_template"/>
        <field name="groups" eval="[(4, ref('base.group_system'))]"/>
        <field name="domain_force">[('model_id.model', 'in', ('calendar.event', 'calendar.attendee'))]</field>
        <field name="perm_read" eval="False"/>
    </record>
</odoo>
